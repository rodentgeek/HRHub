<script>
	
var stateArr = ["AK", "AL", "AR", "AZ", "CA", "CO", "CT", "DC", "DE", "FL", "GA", "HI", "IA", "ID", "IL", "IN", "KS", "KY", "LA", "MA", "MD", "ME", "MI", "MN", "MO", "MS", "MT", "NC", "ND", "NE", "NH", "NM", "NJ", "NV", "NY", "OH", "OK", "OR", "PA", "RI", "SC", "SD", "TN", "TX", "UT", "VA", "VT", "WA", "WI", "WV", "WY"];
  
  // Populate form fields

	document.querySelector("#firstName").value = "<%= data.firstName %>";
	document.querySelector("#lastName").value = "<%= data.lastName %>";
	document.querySelector("#email").value = "<%= data.email %>";
	document.querySelector("#address").value = "<%= data.address %>";
	document.querySelector("#city").value = "<%= data.city %>";
	document.querySelector("#job").value = "<%= data.job %>";
	document.querySelector("#ssn").value = "<%= data.ssn %>";
	document.querySelector("#cNumber").value = "<%= data.cNumber %>";
	document.querySelector("#salary").value = "<%= data.salary %>";
	document.querySelector("#vacayallot").value = "<%= data.vacayallot %>";
	document.querySelector("#vacayused").value = "<%= data.vacayused %>";
	
	// Select correct state drop down
	
	var stateIndex = 0;
	
	stateArr.forEach(function(val, index){
	  if (val == "<%= data.state %>") {
	    stateIndex = index;
	  }
	});
	
	document.forms[0].state[stateIndex].selected = true;
	
</script>